<div class="container text-center">
  <h6 class="page-title mb-4  mt-3 mx-auto bg-secondary border border-primary rounded text-light">
    <b>会員一覧</b>
  </h6>
  <div class="row">
    <div class="col-12 d-flex flex-wrap justify-content-center">
      <table>
        <tbody>

          <tr>
            <th>ユーザー画像</th>
            <th>ユーザ名</th>
          </tr>

          <% @end_users.each do |end_user| %>
            <tr>
              <td>
                <%= link_to admin_end_user_path(end_user) do %>
                  <%= image_tag end_user.get_profile_image, size: "50x50" %>
                <% end %>
              </td>

              <td>
                <%= link_to admin_end_user_path(end_user) do %>
                  <%= end_user.name %>
                <% end %>
              </td>
            </tr>
          <% end %>

        </tbody>
      </table>
    </div>
  </div>
</div>